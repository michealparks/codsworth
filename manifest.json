{
  "version": "0.1.0",
  "manifest_version": 2,
  "author": "Micheal Parks",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "Galeri_48.png",
    "96": "Galeri_96.png",
    "128": "Galeri_128.png"
  },
  "description": "Replace your empty tab page with art masterpieces, updated frequently.",
  "incognito": "spanning",
  "name": "Galeri",
  "permissions": [
    "https://www.google-analytics.com/",
    "storage",
    "activeTab",
    "webRequest"
  ],
  "omnibox": {
    "keyword": "galeri"
  },
  "offline_enabled": true
}
