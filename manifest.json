{
  "version": "0.1.0",
  "manifest_version": 2,
  "author": "Micheal Parks",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "Galeri.svg",
    "96": "Galeri.svg"
  },
  "description": "Replace your empty tab page with art masterpieces, updated frequently.",
  "incognito": "spanning",
  "name": "Galeri",
  "permissions": [
    "activeTab",
    "storage",
    "webRequest"
  ],
  "omnibox": {
    "keyword": "galeri"
  }
}
