{
  "name": "galeri",
  "version": "0.1.1",
  "repository": "michealparks/codsworth",
  "description": "A slim wallpaper app.",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "build-dev": "cross-env NODE_ENV=development rollup -c",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "electron-start": "cross-env NODE_ENV=development electron ./dist/electron.js",
    "electron": "npm-run-all build-dev --parallel dev electron-start",
    "electron-prod": "electron ./dist/electron.js",
    "package": "npm run build && node package"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "archiver": "^3.1.1",
    "cross-env": "^7.0.0",
    "electron": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.31.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0"
  },
  "standard": {
    "globals": [
      "localStorage",
      "XMLHttpRequest",
      "DOMParser",
      "__version__",
      "__dev__",
      "self",
      "caches"
    ]
  },
  "dependencies": {
    "file-type": "^14.0.0",
    "node-fetch": "^2.6.0"
  }
}
