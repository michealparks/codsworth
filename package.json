{
  "name": "codsworth",
  "version": "2.0.3",
  "repository": "michealparks/codsworth",
  "description": "A slim wallpaper app.",
  "license": "MIT",
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development node build.js",
    "build:prod": "cross-env NODE_ENV=production node build.js",
    "dev": "cross-env NODE_ENV=development node build.js",
    "prod": "npm run build && NODE_ENV=production node server.js",
    "build": "cross-env NODE_ENV=production node build.js",
    "deploy:major": "npm version major && npm run build && gh-pages -d public",
    "deploy:minor": "npm version minor && npm run build && gh-pages -d public",
    "deploy:patch": "npm version patch && npm run build && gh-pages -d public"
  },
  "dependencies": {
    "archiver": "^3.0.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "inline-source": "^6.2.0",
    "morgan": "^1.9.1",
    "rollup": "^1.16.6",
    "rollup-plugin-babel-minify": "^9.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sucrase": "^2.1.0",
    "stylus": "^0.54.5"
  },
  "standard": {
    "globals": [
      "localStorage",
      "XMLHttpRequest",
      "DOMParser",
      "__version__",
      "__dev__",
      "self",
      "caches"
    ]
  }
}
