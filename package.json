{
  "name": "codsworth",
  "repository": "michealparks/codsworth",
  "description": "A multipurpose utility app.",
  "license": "",
  "scripts": {
    "inline:dev": "cross-env NODE_ENV=development node inline.js",
    "inline:prod": "cross-env NODE_ENV=production node inline.js",
    "webpack:dev": "cross-env NODE_ENV=development webpack --watch --progress --colors",
    "webpack:prod": "cross-env NODE_ENV=production webpack",
    "styl:dev": "stylus --watch app/index.styl --out public/index.css",
    "styl:prod": "stylus --compress < app/index.styl > public/index.css",
    "build": "npm run webpack:prod && npm run styl:prod && node inline.js",
    "dev": "concurrently \"npm run inline:dev\" \"node server.js\" \"npm run styl:dev\" \"npm run webpack:dev\""
  },
  "dependencies": {},
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-strict-mode": "^6.11.3",
    "babel-preset-es2015": "^6.16.0",
    "babili-webpack-plugin": "0.0.5",
    "concurrently": "^3.1.0",
    "cross-env": "^3.1.2",
    "express": "^4.14.0",
    "gh-pages": "^0.11.0",
    "morgan": "^1.7.0",
    "stylus": "^0.54.5",
    "webpack": "^1.13.2",
    "webpack-validator": "^2.2.9"
  }
}
