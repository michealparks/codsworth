{
  "name": "galeri",
  "version": "0.1.1",
  "repository": "michealparks/codsworth",
  "description": "A slim wallpaper app.",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "NODE_ENV=development rollup -c -w",
    "electron": "npm run dev & electron ./dist/electron.js",
    "electron-prod": "electron ./dist/electron.js",
    "package": "npm run build && node package"
  },
  "devDependencies": {
    "archiver": "^3.1.1",
    "electron": "^7.0.0",
    "rollup": "^1.25.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.2"
  },
  "standard": {
    "globals": [
      "localStorage",
      "XMLHttpRequest",
      "DOMParser",
      "__version__",
      "__dev__",
      "self",
      "caches"
    ]
  }
}
